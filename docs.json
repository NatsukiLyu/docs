                "pages": [
                  "en/documentation/get-started/welcome",
                  "en/documentation/get-started/installation"
                ]
              },
              {
                "group": "Guides",
                "pages": [
                  "essentials/markdown",
                  "essentials/code",
                  "essentials/images",
                  "essentials/settings",
                  "essentials/navigation",
                  "essentials/reusable-snippets"
                ]
              }
            ]
          },
          {
            "tab": "Tools",
            "groups": [
              {
                "group": "API Documentation",
                "pages": [
                  "api-reference/introduction"
                ]
              },
              {
                "group": "Endpoint Examples",
                "pages": [
                  "api-reference/endpoint/get",
                  "api-reference/endpoint/create",
                  "api-reference/endpoint/delete",
                  "api-reference/endpoint/webhook"
                ]
              }
            ]
          }
        ]
      },
      {
        "language": "zh",
        "tabs": [
          {
            "tab": "文档",
            "groups": [
              {
                "group": "开始使用",
                "pages": [
                  "zh/documentation/get-started/welcome",
                  "zh/documentation/get-started/installation"
                ]
              },
